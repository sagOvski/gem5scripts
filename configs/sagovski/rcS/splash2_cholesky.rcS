#!/bin/sh
set -e

cd parsec && echo "changed directory to parsec/"
source env.sh && echo "source'd env.sh"
echo "building splash2.cholesky!! " && parsecmgmt -a build -p splash2.cholesky

# Reset stats
/sbin/m5 dumpresetstats
echo "running splash2.cholesky!! " && parsecmgmt -a run -p splash2.cholesky -i test
echo "Finished running the benchmark"

# Exit
/sbin/m5 exit
