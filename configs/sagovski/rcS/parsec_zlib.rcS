#!/bin/sh
set -e

cd parsec && echo "changed directory to parsec/"
source env.sh && echo "source'd env.sh"
echo "building parsec.zlib!! " && parsecmgmt -a build -p zlib

# Reset stats
/sbin/m5 dumpresetstats
echo "running parsec.zlib!! " && parsecmgmt -a run -p zlib -i simmedium
echo "Finished running the benchmark"

# Exit
/sbin/m5 exit
